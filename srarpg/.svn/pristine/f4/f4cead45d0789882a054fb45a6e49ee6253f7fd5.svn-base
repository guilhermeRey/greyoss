package org.rarpg.info;

public class Spell {
	
	private String name; 
	private int nh; 
	private String damage; 
	private int fatigue;
	
	public Spell(String name, int nh, String damage, int fatigue){
		this.name = name; 
		this.nh = nh; 
		this.damage = damage; 
		this.fatigue = fatigue;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	public void setNh(int nh) {
		this.nh = nh;
	}
	public int getNh() {
		return nh;
	}
	public void setDamage(String damage) {
		this.damage = damage;
	}
	public String getDamage() {
		return damage;
	}
	public void setFatigue(int fatigue) {
		this.fatigue = fatigue;
	}
	public int getFatigue() {
		return fatigue;
	} 
	
}
